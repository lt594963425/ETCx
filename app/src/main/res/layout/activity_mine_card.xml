<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <include layout="@layout/toolbar"/>
    <RelativeLayout
        android:id="@+id/onlian_apply_schedule_toright"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/bg_listview"
        android:descendantFocusability="blocksDescendants">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:paddingLeft="20dp"
            android:paddingRight="15dp"
            android:text="@string/onlian_apply_schedule"
            android:textSize="15sp" />

        <ImageView
            app:srcCompat="@drawable/vd_mine_toright"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:contentDescription="@string/harvestaddress"
            android:focusable="false"
            android:padding="15dp" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />
    <RelativeLayout
        android:id="@+id/etc_card_status_toright"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/bg_listview"
        android:descendantFocusability="blocksDescendants">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:paddingLeft="20dp"
            android:paddingRight="15dp"
            android:text="@string/etc_card_status"
            android:textSize="15sp" />

        <ImageView
            app:srcCompat="@drawable/vd_mine_toright"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:contentDescription="@string/harvestaddress"
            android:focusable="false"
            android:padding="15dp" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />
    <RelativeLayout
        android:id="@+id/binding_card_toright"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/bg_listview"
        android:descendantFocusability="blocksDescendants">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:paddingLeft="20dp"
            android:paddingRight="15dp"
            android:text="@string/binding_card"
            android:textSize="15sp" />
        <ImageView
            app:srcCompat="@drawable/vd_mine_toright"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:contentDescription="@string/harvestaddress"
            android:focusable="false"
            android:padding="15dp" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />
</LinearLayout>